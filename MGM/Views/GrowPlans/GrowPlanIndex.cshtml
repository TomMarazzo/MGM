@model IEnumerable<MGM.Models.GrowPlan>

@{
    ViewData["Title"] = "GrowPlanIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Grow Plan</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New Grow Plan</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.GrowPlanId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.GrowPlanId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.GrowPlanId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
